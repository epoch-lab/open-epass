{
  "name": "epass",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:web": "vite",
    "dev:api": "wrangler pages dev",
    "build": "tsc -b && vite build",
    "deploy": "wrangler pages deploy",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "init-db:local": "wrangler d1 execute epass-db --local --file=./schema.sql",
    "init-db:remote": "wrangler d1 execute epass-db --remote --file=./schema.sql"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@tabler/icons-react": "^3.11.0",
    "@tanstack/react-query": "^5.51.3",
    "@tanstack/react-router": "^1.45.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "jose": "^5.6.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "tailwind-merge": "^2.4.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240712.0",
    "@tanstack/router-devtools": "^1.45.2",
    "@tanstack/router-plugin": "^1.45.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.5",
    "typescript": "^5.5.3",
    "vite": "^5.3.1",
    "vite-plugin-static-copy": "^1.0.6",
    "wrangler": "^3.64.0"
  }
}
